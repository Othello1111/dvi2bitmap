<?xml version="1.0"?>
<!DOCTYPE component SYSTEM "componentinfo.dtd">
<!-- @configure_input@ -->

<component id="@PACKAGE@" support="S">
  <version>@PACKAGE_VERSION@</version>
  <path>applications/dvi2bitmap</path>
  <description>Converts TeX DVI files to bitmaps</description>
  <abstract><p>This application processes a DVI file produced by TeX,
      converting each page to a single bitmap.  The conversion is done
      directly, rather than through a chain of intermediate file formats,
      making the process extremely fast.  It can produce output as
      XBM, XPM, GIF and PNG files.
    </p></abstract>
  <dependencies @STAR_DEPENDENCIES_ATTRIBUTES@>
    @STAR_DEPENDENCIES_CHILDREN@
  </dependencies>
  <developers>
    <person>
      <name>Norman Gray</name>
      <uname>nxg</uname>
      <email>norman@astro.gla.ac.uk</email>
    </person>
  </developers>
  <documentation>@STAR_DOCUMENTATION@</documentation>
  <tests status="yes"/>
  <bugreports>@PACKAGE_BUGREPORT@</bugreports>
  <copyright>
    <year>1992-2004</year>
    <name>Council for the Central Laboratory of the Research Councils</name>
  </copyright>
  <!-- <notes><p></p></notes> -->
</component>
